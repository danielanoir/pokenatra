<h2>New Pokemon</h2>

<form action="/pokemons" method="post">

  <label>Name: </label>
  <input type="text" name="pokemon[name]">

  <label>cp: </label>
  <input type="text" name="pokemon[cp]">

  <label>Poke Type: </label>
  <input type="text" name="pokemon[poke_type]">

  <label>Image: </label>
  <input type="text" name="pokemon[img_url]">
  <input type="submit" value="Create Pokemon">
</form>
